package core;

//1.Arithmetic -> +,-,*,/,%
//2.Conditional -> >,<,>=,<=,!=,==
//3.Logical -> &&,||,!
//4.Assignment -> = , +=,-+,*=,/+
//5.Unary -> ++,--
public class P003_Operators {
	public static void main(String[] args) {
		// 1.arithmetic
		int i = 12, j = 13, k = 45;
		int addition = i+j+k;
		System.out.println(i+j+k);
		System.out.println(addition);
		System.out.println(i-j);
		System.out.println(i*j);
	}
}
