package core;

//1.for(initialization;condition;++/--)
//2.while(condition)
//3.do{} while(condition);
//4.for each
// < -> ++
// > -> --
public class P007_Loops {
	public static void main(String[] args) {
		for (int i = 1; i <= 10; i++) {
			System.out.println(i);
		}
		System.out.println("---------------------");
		int j=1;
		while(j<=10) {
			System.out.println(j);
			j++;
		}
		int k=1;
		do {
			System.out.println(k);
			k++;
		}
	}
}
